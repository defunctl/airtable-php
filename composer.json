{
    "name": "zadorin/airtable-php",
    "description": "Simple PHP wrapper for Airtable API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Zadorin",
            "email": "mail@eugenezadorin.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "davedevelopment/stiphle": "^0.9.2",
        "symfony/polyfill-php80": "^1.25"
    },
    "require-dev": {
        "pestphp/pest": "^1.0",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Zadorin\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
